# ============== BOT SETUP =================

intents = discord.Intents.default()
intents.message_content = True
intents.members = True

bot = commands.Bot(command_prefix="!", intents=intents)

# ============== ESTRELINHAS =================

def carregar_dados():
    if not os.path.exists(ARQUIVO):
        return {}
    with open(ARQUIVO, "r") as f:
        return json.load(f)

def salvar_dados(dados):
    with open(ARQUIVO, "w") as f:
        json.dump(dados, f, indent=4)

# ============== EVENTOS =================

@bot.event
async def on_ready():
    print(f"ğŸ² Monstrinho ligado como {bot.user}")

# ğŸ” SÃ“ O DONO USA COMANDOS
@bot.check
async def somente_dono(ctx):
    return ctx.author.id == DONO_ID

# ============== COMANDOS =================

@bot.command()
async def estrelinha(ctx, membro: discord.Member):
    await ctx.message.delete()
    dados = carregar_dados()
    uid = str(membro.id)
    dados[uid] = dados.get(uid, 0) + 1
    salvar_dados(dados)
    await ctx.send(
        f"â­âœ¨ {membro.mention} ganhou uma estrelinha por ser um monstrinho do bem! ğŸ¥¹ğŸ’š\n"
        f"Agora tem **{dados[uid]}** estrelinhas! â­â­"
    )

@bot.command()
async def ranking(ctx):
    await ctx.message.delete()
    dados = carregar_dados()

    if not dados:
        await ctx.send("ğŸ˜¢ Ainda ninguÃ©m ganhou estrelinhas... bora espalhar amor por aÃ­! â­ğŸ’š")
        return

    texto = (
        "ğŸ†âœ¨ **Ranking de Estrelinhas da CSI** âœ¨ğŸ†\n\n"
        "ğŸŒŸ **Top monstrinhos brilhando:** ğŸŒŸ\n\n"
    )

    ordenado = sorted(dados.items(), key=lambda x: x[1], reverse=True)

    for i, (uid, estrelas) in enumerate(ordenado, start=1):
        membro = ctx.guild.get_member(int(uid))
        nome = membro.name if membro else "UsuÃ¡rio saiu ğŸ˜¢"
        texto += f"{i}. {nome} â€” â­ {estrelas}\n"

    await ctx.send(texto)

@bot.command()
async def remover(ctx, membro: discord.Member):
    await ctx.message.delete()
    dados = carregar_dados()
    uid = str(membro.id)

    if uid in dados:
        del dados[uid]
        salvar_dados(dados)
        await ctx.send(f"âŒ {membro.mention} foi removido do ranking ğŸ˜¢")
    else:
        await ctx.send("ğŸ¤” Essa pessoa nem tava no ranking nÃ£o ğŸ‘€")

@bot.command()
async def clear(ctx):
    await ctx.message.delete()
    salvar_dados({})
    await ctx.send("ğŸ§¹âœ¨ Ranking zerado! Nova era de boas aÃ§Ãµes comeÃ§ando ğŸ˜ˆâ­")

